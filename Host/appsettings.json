{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*",

  "BaseConfiguration": {
    "DatabaseConfig": {
      "DbUserName": "postgres",
      "DbPassword": "postgres",
      "DbConnection": "Host=localhost;Database=yborisovp"
    },
    "SwaggerConfig": {
      "IsEnabled": true,
      "EndpointPrefix": ""
    }
  },
  "CorsConfiguration": {
    "Origins":  ["all"]
  }
}
